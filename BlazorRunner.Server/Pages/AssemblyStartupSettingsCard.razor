<ul class="list-group" style="text-align: left;">
    <li class="list-group-item">
        <button class="btn btn-outline-warning w-100" @onclick="UnloadAll">Unload All</button>
    </li>
    @for (int i = 0; i < AssemblyDirector.StartupAssemblySettings.Count; i++)
    {
        int index = i;

        var pair = AssemblyDirector.StartupAssemblySettings.ElementAt(index);

        Guid id = pair.Key;

        string name = AssemblyDirector.AssemblyInfoDict.Get(id);

        bool previousValue = pair.Value;

        <li class="list-group-item">
            <div class="form-check form-switch">
                <input class="form-check-input" type="checkbox" id="@id" checked="@previousValue" @onclick="()=>SetValue(id,!previousValue)">
                <label class="form-check-label" for="@id">@name</label>
            </div>
            <button class="btn btn-outline-primary w-100" @onclick="()=>Load(id)">Load</button>
        </li>
    }
</ul>
